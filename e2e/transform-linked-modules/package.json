{
  "elric": {
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__",
      "<rootDir>/ignored/",
      "elric-circus",
      "elric-each",
      "elric-environment-node",
      "elric-jasmine2",
      "elric-runner"
    ],
    "transform": {
      "\\.js$": "<rootDir>/preprocessor.js"
    }
  }
}
